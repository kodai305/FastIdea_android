<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Mandara">

    <TextView
            android:text="マンダラチャートの画面"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="88dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="176dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="176dp"
    />
    <FrameLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:id="@+id/container_left"
            app:layout_constraintEnd_toStartOf="@+id/guideline7"
            android:layout_marginEnd="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline9" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline10">

    </FrameLayout>
    <FrameLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/container_center"
            app:layout_constraintStart_toStartOf="@+id/guideline7"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toStartOf="@+id/guideline8"
            android:layout_marginEnd="8dp" app:layout_constraintHorizontal_bias="0.555" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline9" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline10">

    </FrameLayout>

    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline6" app:layout_constraintGuide_begin="160dp"
                                          android:orientation="horizontal" app:layout_constraintGuide_percent="0.25"/>
    <FrameLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:id="@+id/container_right"
            app:layout_constraintStart_toStartOf="@+id/guideline8" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/guideline9"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/guideline10">

    </FrameLayout>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline7" app:layout_constraintGuide_begin="128dp"
                                          android:orientation="vertical" app:layout_constraintGuide_percent="0.333"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline8" app:layout_constraintGuide_begin="262dp"
                                          android:orientation="vertical" app:layout_constraintGuide_percent="0.666"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline9" app:layout_constraintGuide_begin="353dp"
                                          android:orientation="horizontal" app:layout_constraintGuide_percent="0.5"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline10" app:layout_constraintGuide_begin="530dp"
                                          android:orientation="horizontal" app:layout_constraintGuide_percent="0.75"/>
    <FrameLayout
            android:layout_width="0dp"
            android:layout_height="0dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline6" app:layout_constraintEnd_toStartOf="@+id/guideline7"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/container_left" android:id="@+id/container_leftabove">

    </FrameLayout>
    <FrameLayout
            android:layout_width="0dp"
            android:layout_height="0dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline6" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/container_center"
            app:layout_constraintEnd_toStartOf="@+id/guideline8" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="@+id/guideline7" android:layout_marginStart="8dp"
            android:id="@+id/container_avobe">

    </FrameLayout>
    <FrameLayout
            android:layout_width="0dp"
            android:layout_height="0dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline6" app:layout_constraintStart_toStartOf="@+id/guideline8"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/container_right"
            android:id="@+id/container_rightabove">

    </FrameLayout>
    <FrameLayout
            android:layout_width="0dp"
            android:layout_height="0dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline10" android:id="@+id/container_below"
            app:layout_constraintEnd_toStartOf="@+id/guideline8" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="@+id/guideline7" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent">

    </FrameLayout>
    <FrameLayout
            android:layout_width="0dp"
            android:layout_height="0dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toStartOf="@+id/guideline7"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline10" android:id="@+id/container_leftbelow">

    </FrameLayout>
    <FrameLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toEndOf="@+id/container_below"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/guideline10"
            android:id="@+id/container_rightbelow">

    </FrameLayout>


</android.support.constraint.ConstraintLayout>